defaults
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on

account junge
host junge.nl
port 20025
from erwin@junge.nl
user erwin@junge.nl
passwordeval pass junge.nl/erwin

account stamkracht
host smtp.gmail.com
port 587
from erwin@stamkracht.com
user erwin@stamkracht.com
passwordeval pass google.com/erwin@stamkracht.com

account default : junge
